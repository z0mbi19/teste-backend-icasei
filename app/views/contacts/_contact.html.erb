<div id="<%= dom_id contact %>">
  <p>
    <strong>Name:</strong>
    <%= contact.name %>
  </p>

  <p>
    <strong>Birthday:</strong>
    <%= contact.birthday %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= contact.email %>
  </p>

  <p>
    <strong>Mobile:</strong>
    <%= contact.mobile %>
  </p>

  <p>
    <strong>Message:</strong>
    <%= contact.message %>
  </p>

  <p>
    <strong>Advertising:</strong>
    <%= contact.advertising %>
  </p>

  <p>
    <strong>Active:</strong>
    <%= contact.active %>
  </p>

</div>
